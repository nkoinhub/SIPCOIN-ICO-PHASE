        script.
          var stage_current='1';
          var tokens_current=parseFloat(1200000);
          var stage_limit=3000000;
          if(stage_current == '1')
          {
          var percent=(tokens_current/stage_limit) *100;
          var percentage = parseFloat(percent.toFixed(2));
          $(".stage_1_bar").css('width',percentage+'%');
          $("#stage_1_data").html(percentage+"%");
          }
          if(stage_current=='2')
          {
          $(".stage_1_bar").css('width','100%');
          $("#stage_1_data").html("Stage 1 Completed");
          var tokens_for_second_bar=tokens_current-stage_limit;
          var percentage=(tokens_for_second_bar/stage_limit) *100;
          percentage = parseFloat(percentage.toFixed(2));
          $(".stage_2_bar").css('width',percentage+'%');
          $("#stage_2_data").html(percentage+"%");
          }
          if(stage_current=='3')
          {
          $(".stage_1_bar").css('width','100%');
          $(".stage_2_bar").css('width','100%');
          $("#stage_1_data").html("Stage 1 Completed");
          $("#stage_2_data").html("Stage 2 Completed");
          var tokens_for_third_bar=tokens_current-stage_limit*2;
          var percentage=(tokens_for_third_bar/stage_limit) *100;
          percentage = parseFloat(percentage.toFixed(2));
          $(".stage_3_bar").css('width',percentage+'%');
          $("#stage_3_data").html(percentage+"%");
          }

